//POST

//ORGANIZER

{
  "$class": "org.hyperledger_composer.ballots.organizers",
  "organizerId": "string" //unique key paired to authentication
}

//VOTER
{
  "$class": "org.hyperledger_composer.ballots.voters",
  "voterId": "string" //unique key paired to authentication
}

//ELECTIONS

{
  "$class": "org.hyperledger_composer.ballots.elections",
  "electionId": "string", //unique key
  "organizer": {}, //organizerId string in system eg. elect000001
  "propositions": [//each object in represents a diffrent question to be answered
    {
      "$class": "org.hyperledger_composer.ballots.proposition",
      "question": "string",//plain text question or description to be displayed to voters
      "choices": [//Each string is a valid choice for the propsition
        "string"
      ],
      "id": "string"//unique key, should have the key of the parent election at the front eg elect000001q001
    }
  ],
  "startDate": "2018-01-30T18:14:17.269Z",//both times of day should be same
  "endDate": "2018-01-30T18:14:17.269Z"//times should be at the nearest hour +/- a minute to avoid noon midnight confusion
}

//Transactions

//SUBSCRIBE

{//Creates an unmarked ballot linked to the given voter for the given elections, will not work if this pair already has a ballot
  "$class": "org.hyperledger_composer.ballots.subscribe",
  "voter": {}, //voter ID string
  "election": {}, //election ID string
  "transactionId": "string", //unique key
  "timestamp": "2018-01-30T18:14:17.396Z"
}


//VOTE

[//can handle multiple ballots
  {//each ballot must have marked = false and will have it set to true after the transaction
    "$class": "org.hyperledger_composer.ballots.vote",
    "unmarkedBallot": {},//ballot Id string (not the subscribe id)
    "answers": [//an array of array indexes
      0,//order of numbers in the array corrisponds to order of questions
      0//each number ocr
    ],
    "transactionId": "string",
    "timestamp": "2018-01-30T18:14:17.453Z"
  }
]
